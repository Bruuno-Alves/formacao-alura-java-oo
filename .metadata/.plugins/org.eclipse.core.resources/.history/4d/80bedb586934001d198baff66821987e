
public class Fatorial {

	public static void main(String[] args) {

		for (int i = 0; i <= 10; i++) {
			System.out.printf("Fatorial de % ! é : ", i);
			for (int fat = 1; fat <= i; fat++) {
				if (fat < i) {
					System.out.print((i + " x "));
				} else {
					System.out.print( i + " = ");
				}
			}
		}
	}

}
